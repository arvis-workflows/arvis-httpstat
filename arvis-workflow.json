{
  "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
  "category": "Tools",
  "name": "httpstat",
  "readme": "",
  "creator": "leozhang2018",
  "description": "httpstat visualizes",
  "defaultIcon": "icon.png",
  "version": "0.0.1",
  "webAddress": "https://github.com/jopemachine/arvis-httpstat",
  "enabled": true,
  "commands": [
    {
      "type": "scriptFilter",
      "command": "hs",
      "title": "httpstat visualizes",
      "subtitle": "",
      "scriptFilter": "node index.js \"{query}\"",
      "runningSubtext": "Searching...",
      "withspace": true,
      "argType": "required",
      "actions": [
        {
          "type": "clipboard",
          "modifiers": "normal",
          "text": "{query}"
        }
      ]
    }
  ]
}
